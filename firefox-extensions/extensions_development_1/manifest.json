{
    "manifest_version": 2,
    "name": "DDG Position",
    "version": "1.0",

    "description": "Places a marker with the position of a GOOGLE result Link in DuckDuckGo Results.",
    "content_script": [
        {
            "matches": ["*://*.google.com/search?*"],
            "js": ["ddgposition.js"]
        }
    ],

    "permissions": [
        "*://duckduckgo.com/html/*"
    ]




}